<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>


<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.library_managmenrt.controller.DashboardController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>


    <Button text="Category" fx:id="category_button" onAction="#handleCategoryClick"/>
    <Button text="Books" fx:id="book_button" onAction="#handleBookClick"/>
    <Button text="Members" fx:id="member_button" onAction="#handleMemberClick"/>
    <Button text="Borrowings" fx:id="transaction_button" onAction="#handleBorrowingClick"/>



</VBox>
